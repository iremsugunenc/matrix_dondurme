matrix = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]
print(" Matris:")
for satir in matrix:
    print(satir)
yeni_matrix= [
    [matrix[0][2], matrix[1][2], matrix[2][2]],  
    [matrix[0][1], matrix[1][1], matrix[2][1]],
    [matrix[0][0], matrix[1][0], matrix[2][0]]  
]
print("90 derece sola döndürülmüş matris:")
for satir in yeni_matrix:
    print(satir)
